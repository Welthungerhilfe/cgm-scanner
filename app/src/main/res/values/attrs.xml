<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckboxView">
        <attr name="checkboxText" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="LanguageRadioView">
        <attr name="flagImage" format="reference" />
        <attr name="language" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="ScanType">
        <attr name="scantypecolor" format="color|reference" />
        <attr name="scantypeicon" format="integer" />
        <attr name="scantypetext" format="string|reference" />
        <attr name="scantypetitle" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="ScanMode">
        <attr name="scanmodeactive" format="boolean" />
        <attr name="scanmodeicon" format="integer" />
        <attr name="scanmodetext" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="TwoLineTextView">
        <attr name="titleText" format="string|reference" />
        <attr name="titleTextColor" format="color|reference" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="descriptionText" format="string|reference" />
        <attr name="checkVisible" format="boolean" />
        <attr name="separatorVisible" format="boolean" />
        <attr name="submenuVisible" format="boolean" />
        <attr name="toggleVisible" format="boolean" />
        <attr name="valueVisible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="UnitEditText">
        <attr name="unit" format="string|reference" />
        <attr name="unitPadding" format="dimension" />
        <attr name="unitMargin" format="dimension" />
    </declare-styleable>
</resources>