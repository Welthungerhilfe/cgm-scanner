        <attr name="flagImage" format="reference" />
        <attr name="language" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="SubmenuButton">
        <attr name="submenuText" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="ToggleView">
        <attr name="toggleText" format="string|reference" />
